{
    "env": {
        "browser": false,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "project": "./tsconfig.json",
        "tsconfigRootDir": "./"
    },
    "plugins": [
        "@typescript-eslint",
        "import"
    ],
    "rules": {
        "import/extensions": "off",
        "import/no-unresolved": "off",
        "no-debugger": "warn"
    },
    "settings": {
        "import/resolver": {
            "typescript": {}
        }
    }
}
